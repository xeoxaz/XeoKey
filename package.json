{
  "name": "xeokey",
  "version": "1.0.0",
  "description": "A secure, self-hosted password manager built with Bun and MongoDB",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "cd src && bun run dev",
    "start": "cd src && bun run start",
    "typecheck": "cd src && bun run typecheck",
    "test": "cd src && bun test",
    "test:unit": "cd src && bun test tests/unit",
    "test:integration": "cd src && bun test tests/integration",
    "test:watch": "cd src && bun test --watch",
    "test:coverage": "cd src && bun test --coverage"
  },
  "engines": {
    "bun": ">=1.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xeoxaz/XeoKey.git"
  },
  "bugs": {
    "url": "https://github.com/xeoxaz/XeoKey/issues"
  },
  "homepage": "https://github.com/xeoxaz/XeoKey#readme",
  "license": "ISC",
  "author": "xeoxaz"
}

