{
  "name": "xeokey-server",
  "version": "1.0.0",
  "description": "A secure, self-hosted password manager built with Bun and MongoDB. Features password encryption, analytics tracking, and a modern web interface.",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "dev": "bun run --watch server.ts",
    "start": "bun run server.ts",
    "typecheck": "bun --bun tsc --noEmit",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage"
  },
  "keywords": [
    "password-manager",
    "security",
    "bun",
    "mongodb",
    "self-hosted",
    "typescript",
    "encryption",
    "authentication",
    "web-application",
    "analytics"
  ],
  "author": "xeoxaz",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/xeoxaz/XeoKey.git"
  },
  "bugs": {
    "url": "https://github.com/xeoxaz/XeoKey/issues"
  },
  "homepage": "https://github.com/xeoxaz/XeoKey#readme",
  "engines": {
    "bun": ">=1.3.4"
  },
  "dependencies": {
    "@frostal/monitor": "github:xeoxaz/Monitor",
    "bcryptjs": "^3.0.3",
    "chart.js": "^4.5.1",
    "mongodb": "^7.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^25.0.2"
  }
}
